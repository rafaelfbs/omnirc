  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:esm",
    "build-storybook": "build-storybook",
    "build:clean": "rimraf ./dist && rimraf ./lib",
    "build:esm": "cross-env NODE_ENV=production tsc",
    "start": "npm run watch",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:ci": "jest --ci --coverage --env=jsdom --reporters=default --reporters=\"jest-junit\"",
    "watch": "cross-env NODE_ENV=development npm run build:clean && npm-run-all watch:*",
    "watch:source": "cross-env NODE_ENV=development tsc -w --preserveWatchOutput"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@storybook/addon-actions": "^5.0.3",
