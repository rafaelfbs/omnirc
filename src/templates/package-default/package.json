    "ts-jest": "^24.0.2",
    "typescript": "3.2.1"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0",
    "react-redux": ">= 7.1.0",
    "react-router-dom": ">= 5.0.1",
    "redux": ">= 4.0.4"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./test/junit.xml",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  }
}
